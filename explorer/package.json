{
  "name": "explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:lungmap": "cp ./site-config/lungmap/dev/.env .env.development && next dev",
    "build:lungmap": "cp ./site-config/lungmap/prod/.env .env.production && next build && next export",
    "dev:anvil": "cp ./site-config/anvil/dev/.env .env.development && next dev",
    "build:anvil": "cp ./site-config/anvil/prod/.env .env.production && next build && next export",
    "dev:hca-dcp": "cp ./site-config/hca-dcp/dev/.env .env.development && next dev",
    "build:hca-dcp": "cp ./site-config/hca-dcp/prod/.env .env.production && next build && next export",
    "start": "npx serve out",
    "lint": "next lint --dir .",
    "check-format": "prettier --check .",
    "prepare": "cd .. && husky install explorer/.husky",
    "storybook": "start-storybook -s images,.next  -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "copy-to-clipboard": "^3.3.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "next": "12.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-table": "^7.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@next/eslint-plugin-next": "^12.1.6",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-table": "^7.7.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "babel-loader": "^8.2.5",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-storybook": "^0.5.11",
    "husky": "^8.0.0",
    "prettier": "2.7.1",
    "typescript": "4.6.4"
  }
}
