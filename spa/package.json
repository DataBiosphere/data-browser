{
  "name": "data-browser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --verbose",
    "build": "ng build --prod  ---outputPath ../dist",
    "build-develop": "ng build --prod  --outputPath ../dist/explore --configuration=develop  --base-href=/explore/",
    "build-cgl-dev": "ng build --prod  --outputPath ../dist/explore --configuration=cgl-dev --base-href=/explore/",
    "build-lungmap-dev": "ng build --prod  --outputPath ../dist/explore --configuration=lungmap-dev --base-href=/explore/",
    "build-ux-dev": "ng build --prod  --outputPath ../dist/explore --configuration=ux-dev --base-href=/explore/",
    "build-integration": "ng build --prod  --outputPath ../dist/explore --configuration=integration --base-href=/explore/",
    "build-staging": "ng build --prod  --outputPath ../dist/explore --configuration=staging --base-href=/explore/",
    "build-cgl-dcp2": "ng build --prod  --outputPath ../dist/explore --configuration=dcp2 --base-href=/explore/",
    "build-prod": "ng build --prod  --outputPath ../dist/explore --configuration=production --base-href=/explore/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "contributors": "node ../script/release-contributors.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng test --watch=false"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.12",
    "@angular/cdk": "9.2.4",
    "@angular/common": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/core": "9.1.12",
    "@angular/forms": "9.1.12",
    "@angular/material": "9.2.4",
    "@angular/platform-browser": "9.1.12",
    "@angular/platform-browser-dynamic": "9.1.12",
    "@angular/platform-server": "9.1.12",
    "@angular/router": "9.1.12",
    "@ngrx/effects": "9.2.0",
    "@ngrx/store": "9.2.0",
    "core-js": "2.4.1",
    "lodash-es": "4.17.15",
    "ngx-clipboard": "13.0.1",
    "ngx-device-detector": "1.4.5",
    "rxjs": "6.6.0",
    "tslib": "1.13.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.11",
    "@angular/cli": "9.1.11",
    "@angular/compiler-cli": "9.1.12",
    "@angular/language-service": "9.1.12",
    "@ngrx/store-devtools": "9.2.0",
    "@types/jasmine": "3.5.11",
    "@types/jasminewd2": "2.0.8",
    "bufferutil": "4.0.1",
    "codelyzer": "6.0.0",
    "fibers": "5.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jasmine-core": "3.5.0",
    "jasmine-marbles": "0.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "node-sass": "4.14.1",
    "protractor": "7.0.0",
