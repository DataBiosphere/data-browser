{
  "name": "data-browser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --verbose",
    "build": "ng build --prod  ---outputPath ../dist",
    "build-develop": "ng build --prod  --outputPath ../dist/explore --configuration=develop  --base-href=/explore/",
    "build-ux-dev": "ng build --prod  --outputPath ../dist/explore --configuration=ux-dev --base-href=/explore/",
    "build-integration": "ng build --prod  --outputPath ../dist/explore --configuration=integration --base-href=/explore/",
    "build-staging": "ng build --prod  --outputPath ../dist/explore --configuration=staging --base-href=/explore/",
    "build-prod": "ng build --prod  --outputPath ../dist/explore --configuration=production --base-href=/explore/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "contributors": "node ../script/release-contributors.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng test --watch=false"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.4",
    "@angular/cdk": "8.1.4",
    "@angular/common": "8.2.4",
    "@angular/compiler": "8.2.4",
    "@angular/core": "8.2.4",
    "@angular/forms": "8.2.4",
    "@angular/material": "8.1.4",
    "@angular/platform-browser": "8.2.4",
    "@angular/platform-browser-dynamic": "8.2.4",
    "@angular/platform-server": "8.2.4",
    "@angular/router": "8.2.4",
    "@ngrx/effects": "7.4.0",
    "@ngrx/store": "7.4.0",
    "core-js": "2.4.1",
    "lodash": "4.17.15",
    "ngx-clipboard": "12.2.1",
    "ngx-device-detector": "1.3.11",
    "rxjs": "6.5.3",
    "tslib": "1.9.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.25",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "8.2.4",
    "@angular/language-service": "8.2.4",
    "@ngrx/store-devtools": "7.4.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "4.14.133",
    "codelyzer": "5.0.1",
    "husky": "3.0.3",
    "jasmine-core": "2.99.1",
    "jasmine-marbles": "0.6.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.4.0",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.5.3"
  }
}
