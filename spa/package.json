{
  "name": "temp-spa",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -v --proxy-config proxy.conf.json",
    "build": "ng build --prod -aot false -op ../dist",
    "build-develop": "ng build --prod -aot false -op ../dist/explore --env=develop --base-href=/explore/",
    "build-ux-dev": "ng build --prod -aot false -op ../dist/explore --env=ux-dev --base-href=/explore/",
    "build-integration": "ng build --prod -aot false -op ../dist/explore --env=integration --base-href=/explore/",
    "build-staging": "ng build --prod -aot false -op ../dist/explore --env=staging --base-href=/explore/",
    "build-prod": "ng build --prod -aot false -op ../dist/explore --env=prod --base-href=/explore/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/cdk": "^5.0.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/material": "^5.0.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/platform-server": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "core-js": "2.4.1",
    "d3": "4.10.2",
    "lodash": "4.17.11",
    "ngx-clipboard": "10.0.0",
    "ngx-device-detector": "^1.3.4",
    "rxjs": "^5.5.5",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "4.3.2",
    "@ngrx/store-devtools": "^4.0.0",
    "@types/d3": "^4.10.0",
    "@types/lodash": "^4.14.74",
    "typescript": "2.4.2"
  }
}
