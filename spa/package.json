{
  "name": "data-browser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --verbose",
    "build": "ng build --prod  ---outputPath ../dist",
    "build-develop": "ng build --prod  --outputPath ../dist/explore --configuration=develop  --base-href=/explore/",
    "build-ux-dev": "ng build --prod  --outputPath ../dist/explore --configuration=ux-dev --base-href=/explore/",
    "build-integration": "ng build --prod  --outputPath ../dist/explore --configuration=integration --base-href=/explore/",
    "build-staging": "ng build --prod  --outputPath ../dist/explore --configuration=staging --base-href=/explore/",
    "build-prod": "ng build --prod  --outputPath ../dist/explore --configuration=production --base-href=/explore/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.8",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "7.2.8",
    "@angular/compiler": "7.2.8",
    "@angular/core": "7.2.8",
    "@angular/forms": "7.2.8",
    "@angular/http": "7.2.8",
    "@angular/material": "7.3.3",
    "@angular/platform-browser": "7.2.8",
    "@angular/platform-browser-dynamic": "7.2.8",
    "@angular/platform-server": "7.2.8",
    "@angular/router": "7.2.8",
    "@ngrx/effects": "7.3.0",
    "@ngrx/store": "7.3.0",
    "core-js": "2.4.1",
    "lodash": "4.17.11",
    "ngx-clipboard": "10.0.0",
    "ngx-device-detector": "^1.3.4",
    "rxjs": "6.4.0",
    "tslib": "1.9.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "7.2.8",
    "@angular/language-service": "7.2.8",
    "@ngrx/store-devtools": "^7.4.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "^4.14.133",
    "codelyzer": "4.5.0",
    "jasmine-core": "2.99.1",
    "jasmine-marbles": "0.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.4.0",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.1.1"
  }
