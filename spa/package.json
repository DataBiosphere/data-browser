{
  "name": "data-browser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --verbose",
    "build": "ng build --outputPath ../dist",
    "build-cgl-dev": "ng build --outputPath ../dist/explore --configuration=cgl-dev --base-href=/explore/",
    "build-cgl-ux-dev": "ng build --outputPath ../dist/explore --configuration=cgl-ux-dev --base-href=/explore/",
    "build-lungmap-dev": "ng build --outputPath ../dist/explore --configuration=lungmap-dev --base-href=/explore/",
    "build-lungmap-prod": "ng build --outputPath ../dist/explore --configuration=lungmap-prod --base-href=/explore/",
    "build-cgl-dcp2": "ng build --outputPath ../dist/explore --configuration=dcp2 --base-href=/explore/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update-archive-previews": "cd scripts && node update-archive-previews.js",
    "update-sitemap": "cd scripts && node update-sitemap.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng test --watch=false --browsers ChromeHeadless"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.1",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "^13.3.1",
    "@angular/compiler": "^13.3.1",
    "@angular/core": "^13.3.1",
    "@angular/forms": "^13.3.1",
    "@angular/material": "^15.1.0",
    "@angular/platform-browser": "^13.3.1",
    "@angular/platform-browser-dynamic": "^13.3.1",
    "@angular/platform-server": "^13.3.1",
    "@angular/router": "^13.3.1",
    "@ngrx/effects": "^13.1.0",
    "@ngrx/store": "^13.1.0",
    "lodash-es": "^4.17.21",
    "ngx-clipboard": "^15.1.0",
    "ngx-device-detector": "^2.2.1",
    "rxjs": "^6.6.7",
    "save-dev": "0.0.1-security",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.1",
    "@angular-eslint/builder": "^13.2.0",
    "@angular-eslint/eslint-plugin": "^13.2.0",
    "@angular-eslint/eslint-plugin-template": "^13.2.0",
    "@angular-eslint/schematics": "^13.2.0",
    "@angular-eslint/template-parser": "^13.2.0",
    "@angular/cli": "^13.3.1",
    "@angular/compiler-cli": "^13.3.1",
    "@angular/language-service": "^13.3.1",
    "@ngrx/store-devtools": "^13.1.0",
    "@types/gapi": "^0.0.41",
    "@types/gapi.auth2": "^0.0.56",
    "@types/jasmine": "^4.0.2",
    "@types/jasminewd2": "2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "bufferutil": "^4.0.6",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fibers": "^5.0.1",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jasmine-core": "^4.0.1",
    "jasmine-marbles": "^0.8.4",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "ts-node": "8.10.2",
    "typescript": "^4.6.3",
    "utf-8-validate": "^5.0.9",
    "ws": "^7.5.7"
  }
}
