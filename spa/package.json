{
  "name": "data-browser",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --verbose",
    "build": "ng build --outputPath ../dist",
    "build-cgl-dev": "ng build --outputPath ../dist/explore --configuration=cgl-dev --base-href=/explore/",
    "build-cgl-ux-dev": "ng build --outputPath ../dist/explore --configuration=cgl-ux-dev --base-href=/explore/",
    "build-lungmap-dev": "ng build --outputPath ../dist/explore --configuration=lungmap-dev --base-href=/explore/",
    "build-lungmap-prod": "ng build --outputPath ../dist/explore --configuration=lungmap-prod --base-href=/explore/",
    "build-cgl-dcp2": "ng build --outputPath ../dist/explore --configuration=dcp2 --base-href=/explore/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng test --watch=false --browsers ChromeHeadless"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.10",
    "@angular/cdk": "^12.2.10",
    "@angular/common": "^12.2.10",
    "@angular/compiler": "^12.2.10",
    "@angular/core": "^12.2.10",
    "@angular/forms": "^12.2.10",
    "@angular/material": "^12.2.10",
    "@angular/platform-browser": "^12.2.10",
    "@angular/platform-browser-dynamic": "^12.2.10",
    "@angular/platform-server": "^12.2.10",
    "@angular/router": "^12.2.10",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "core-js": "^2.6.12",
    "lodash-es": "^4.17.21",
    "ngx-clipboard": "^14.0.1",
    "ngx-device-detector": "^2.1.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.10",
    "@angular/cli": "^12.2.10",
    "@angular/compiler-cli": "^12.2.10",
    "@angular/language-service": "^12.2.10",
    "@ngrx/store-devtools": "^12.4.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.54",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "bufferutil": "4.0.4",
    "codelyzer": "^6.0.2",
    "fibers": "5.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "8.10.2",
    "typescript": "4.3.5",
